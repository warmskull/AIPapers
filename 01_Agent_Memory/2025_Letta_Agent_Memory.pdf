<!-- PIXEL GRID ANIMATION — one-shot at 10%, ultra-light ImageData + buffer reuse (stabilized on refresh) -->
<script>
/* ================== Canvas & perf settings (unchanged API/params) ================== */
const canvas = document.getElementById('pixel-grid');
const ctx = canvas.getContext('2d');
const INTERNAL_SCALE = 0.5;
const MAX_DPR = 1;
const FLICKER_FPS = 1;   // kept for compatibility
const MIN_DENSITY = 0.7; // kept for compatibility
const MAX_DENSITY = 0.01;// kept for compatibility

/* Offscreen buffer (1 pixel per cell) */
const off = document.createElement('canvas');
const offctx = off.getContext('2d', { alpha: true });
let imgData = null;          // ImageData(cols x rows)
let imgBuf8 = null;          // Uint8ClampedArray view
let imgBuf32 = null;         // Uint32Array view (fast writes)

/* Color probe */
let probe = null;
function ensureProbe() {
  if (probe) return probe;
  probe = document.createElement('div');
  probe.style.position = 'fixed';
  probe.style.left = '-9999px';
  probe.style.top = '0';
  probe.style.width = '1px';
  probe.style.height = '1px';
  probe.style.pointerEvents = 'none';
  probe.style.backgroundColor = 'var(--color--background-blog)';
  document.body.appendChild(probe);
  return probe;
}
function resolvePixelColor() { return getComputedStyle(ensureProbe()).backgroundColor; }

/* Layout state */
let p = 12, cols = 0, rows = 0, dpr = 1;

/* Reveal state */
let order = null;            // Uint32Array shuffled indices
let revealCount = 0;
let prevRevealCount = 0;

/* Trigger (one-shot) */
const wrapper = document.querySelector('.pixel-grid_wrapper');
let thresholdY = 0;
let hasPlayed = false;

/* Animation */
let isAnimating = false;
let animFrame = 0;
let animStart = 0;
let animFrom = 0;
let animTo = 0;
const ANIM_MS = 500;
let lastAnimTs = 0;          // ~60fps cap

/* Recompute deferral during anim (iOS address bar) */
let pendingRecompute = false;
let animSoftFreezeUntil = 0; // allow one immediate recompute for first 250ms

/* Theme color (snap instantly) */
let rgb = [0,0,0];
let packedOn = 0;            // 0xAABBGGRR (little-endian)

/* ===== Helpers ===== */
function getPixelSizeCSS() {
  const v = getComputedStyle(document.documentElement).getPropertyValue('--pixel-size').trim();
  const parsed = parseFloat(v);
  return Number.isNaN(parsed) ? 24 : parsed;
}
function pageTop(el){ const r = el.getBoundingClientRect(); return r.top + window.scrollY; }
function buildOrder(total){
  order = new Uint32Array(total);
  for (let i=0;i<total;i++) order[i]=i;
  for (let i=total-1;i>0;i--){ const j = Math.floor(Math.random()*(i+1)); const t=order[i]; order[i]=order[j]; order[j]=t; }
}
function parseRGB(css){
  const m = css.match(/\d+/g);
  if (!m) return [0,0,0];
  return [m[0]|0, m[1]|0, m[2]|0];
}
function updatePackedColor(){
  // Little-endian canvas: Uint32 writes expect AABBGGRR
  packedOn = (255 << 24) | (rgb[2] << 16) | (rgb[1] << 8) | (rgb[0]);
}

/* Trigger at 10% wrapper height */
function computeScrollRange(){
  if (!wrapper) return;
  const top = pageTop(wrapper);
  const wh  = wrapper.getBoundingClientRect().height;
  thresholdY = top + 0.01 * wh;
}

/* ===== Layout & buffers (robust sizing; reuse when possible) ===== */
function computeLayout() {
  const savedReveal = revealCount;

  dpr = Math.min(window.devicePixelRatio || 1, MAX_DPR);

  // Prefer the wrapper box for stable CSS size if canvas returns tiny/zero
  const box = canvas.getBoundingClientRect();
  let cssW = Math.max(box.width, 1);
  let cssH = Math.max(box.height, 1);
  if (cssH <= 1 && wrapper) {
    const wb = wrapper.getBoundingClientRect();
    cssW = Math.max(wb.width, 1);
    cssH = Math.max(wb.height, 1);
  }

  // Offscreen "design space"
  const offW_px = Math.ceil(cssW * INTERNAL_SCALE);
  const offH_px = Math.ceil(cssH * INTERNAL_SCALE);

  // Cell size in that space
  const cssPixel = getPixelSizeCSS();
  p = Math.max(1, Math.round(cssPixel * INTERNAL_SCALE));

  // Grid resolution in cells
  const newCols = Math.max(1, Math.ceil(offW_px / p));
  const newRows = Math.max(1, Math.ceil(offH_px / p));

  // Visible canvas size in device pixels (DPR-capped)
  const outW = Math.ceil(cssW * dpr);
  const outH = Math.ceil(cssH * dpr);

  // Always set visible canvas size and CSS size
  canvas.width = outW;
  canvas.height = outH;
  canvas.style.width = '100%';
  canvas.style.height = '100%';

  const sizeChanged = (newCols !== cols) || (newRows !== rows);

  // Reallocate only if grid size actually changed
  if (sizeChanged) {
    cols = newCols;
    rows = newRows;
    off.width = cols; 
    off.height = rows;

    imgData  = offctx.createImageData(cols, rows);
    imgBuf8  = imgData.data;
    imgBuf32 = new Uint32Array(imgBuf8.buffer);

    buildOrder(cols * rows);
    revealCount = Math.min(savedReveal, cols * rows);
    prevRevealCount = 0;
  } else {
    revealCount = Math.min(savedReveal, cols * rows);
    // imgData/order reused 1:1
  }

  // Snap color
  rgb = parseRGB(resolvePixelColor());
  updatePackedColor();

  // Paint current state
  fullRepaintFromReveal();
  blit();

  computeScrollRange();
}

/* ===== Ultra-light painter ===== */
function fullRepaintFromReveal(){
  // Clear fully
  imgBuf8.fill(0);
  // Set revealed cells with one 32-bit write per pixel
  for (let i=0;i<revealCount;i++) imgBuf32[order[i]] = packedOn;
  offctx.putImageData(imgData, 0, 0);
}
function deltaRepaint(prevCount){
  if (revealCount > prevCount) {
    for (let i = prevCount; i < revealCount; i++) imgBuf32[order[i]] = packedOn;
  } else if (revealCount < prevCount) {
    for (let i = revealCount; i < prevCount; i++) imgBuf32[order[i]] = 0;
  } else {
    return; // no changes
  }
  offctx.putImageData(imgData, 0, 0);
}
function blit(){
  // Single nearest-neighbor scale, no extra clears
  ctx.setTransform(1,0,0,1,0,0);
  ctx.imageSmoothingEnabled = false;
  canvas.style.imageRendering = 'pixelated';
  ctx.globalCompositeOperation = 'copy'; // cheaper compositing on WebKit
  ctx.drawImage(off, 0,0, off.width,off.height, 0,0, canvas.width,canvas.height);
  ctx.globalCompositeOperation = 'source-over';
}

/* ===== One-shot animation (1s), ~60fps cap ===== */
function startAnimationOnce(){
  if (hasPlayed) return;

  // Pre-flight: recompute layout & threshold right before starting,
  // so we never start on a half-built layout after refresh-in-middle.
  computeLayout();
  computeScrollRange();

  hasPlayed = true;
  isAnimating = true;
  if (animFrame) { cancelAnimationFrame(animFrame); animFrame = 0; }

  animFrom = revealCount;
  animTo   = cols * rows;
  animStart = performance.now();
  animSoftFreezeUntil = animStart + 250; // let one early recompute pass if layout still stabilizes
  lastAnimTs = 0;

  // Lock color for duration
  rgb = parseRGB(resolvePixelColor());
  updatePackedColor();

  const step = (now) => {
    // ~60fps cap (skip frames on 120Hz)
    if (lastAnimTs && now - lastAnimTs < 16) { animFrame = requestAnimationFrame(step); return; }
    lastAnimTs = now;

    const prev = revealCount;
    const t = Math.min(1, (now - animStart) / ANIM_MS);
    revealCount = Math.round(animFrom + (animTo - animFrom) * t);

    deltaRepaint(prev);
    blit();

    if (t < 1) {
      animFrame = requestAnimationFrame(step);
    } else {
      animFrame = 0;
      isAnimating = false;

      // Final full repaint guarantees 100% revealed visible everywhere
      revealCount = cols * rows;
      fullRepaintFromReveal();
      blit();

      if (pendingRecompute) { pendingRecompute = false; computeLayout(); fullRepaintFromReveal(); blit(); }
      window.removeEventListener('scroll', onScroll, passiveScrollOpts);
    }
  };
  animFrame = requestAnimationFrame(step);
}

/* ===== Scroll trigger (one-shot at 10%) ===== */
function onScroll(){
  if (!wrapper || hasPlayed) return;
  if (window.scrollY >= thresholdY) startAnimationOnce();
}

/* ===== Theme change: snap instantly, cheap repaint; reuse buffers ===== */
function onThemeChange(){
  const newRGB = parseRGB(resolvePixelColor());
  if (newRGB[0] === rgb[0] && newRGB[1] === rgb[1] && newRGB[2] === rgb[2]) return;
  rgb = newRGB;
  updatePackedColor();
  fullRepaintFromReveal();
  blit();
}
['transitionrun','transitionstart'].forEach(e=>{
  document.addEventListener(e, onThemeChange, true);
});
new MutationObserver(onThemeChange)
  .observe(document.documentElement, { attributes:true, attributeFilter:['class','data-theme'] });

/* ===== Resize: allow one early recompute during first 250ms of animation; otherwise defer ===== */
function recompute(){ computeLayout(); }
const passiveScrollOpts = { passive:true };
window.addEventListener('resize', ()=>{
  if (isAnimating) {
    if (performance.now() <= animSoftFreezeUntil) {
      // allow a single early recompute to avoid huge tiles at startup
      recompute();
    } else {
      pendingRecompute = true;
      return;
    }
  } else {
    recompute();
  }
  computeScrollRange();
  if (!hasPlayed) onScroll();
}, { passive:true });
new ResizeObserver(()=>{
  if (isAnimating) {
    if (performance.now() <= animSoftFreezeUntil) {
      recompute();
    } else {
      pendingRecompute = true;
      return;
    }
  } else {
    recompute();
  }
  computeScrollRange();
  if (!hasPlayed) onScroll();
}).observe(canvas.parentElement || canvas);

/* ===== Start ===== */
computeLayout();
computeScrollRange();
window.addEventListener('scroll', onScroll, passiveScrollOpts);
onScroll();
</script></body></html>